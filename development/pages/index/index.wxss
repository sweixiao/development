/**index.wxss**/
.index{
  padding: 15px;
}
.header-auto{
  display: flex;
  justify-content:center;
  align-items: center;
  margin-bottom:15px ;
}
.clock{
  width: 80px;
  height: 80px;
  line-height: 80px;
  text-align:center;
  border-radius:50%;
  margin-right: 25px;
  color: #fff;
  background: linear-gradient(to bottom,rgb(97, 233, 97),rgb(22, 182, 22));
}
.userImg image{
  width: 30px;
  height: 30px;
  border-radius: 50%;
  object-fit: fill;
  border: 1px solid #ccc;
  margin-bottom:5px ;
}
.main{
  border-top: 2px solid #ccc;
}
.sujustClass{
  padding-top: 10px !important;
}